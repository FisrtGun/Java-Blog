        function getCookie(name) {
            var cookie = document.cookie;
            var a = cookie.split("; ");
            for (var j = 0, m = a.length; j < m; j++) {
                var b = a[j].split("=");
                if (b[0].toLowerCase() == name) {
                    return b[1];
                    break;
                }
            }
        }
    });
